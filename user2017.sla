        <PAGEOBJECT XPOS="631.98688" YPOS="2486" OwnPage="0" ItemID="163079760" PTYPE="4" WIDTH="399.98688" HEIGHT="804.392" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="2" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L399.987 0 L399.987 804.392 L0 804.392 L0 0 Z" copath="M0 0 L399.987 0 L399.987 804.392 L0 804.392 L0 0 Z" gXpos="631.98688" gYpos="2486" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Statistical model"/>
                <para PARENT="Section heading"/>
                <ITEXT CH="Given a set of paired comparisons, the "/>
                <ITEXT FONT="Goudy Old Style Bold" CH="Bradley–Terry model"/>
                <ITEXT CH=" estimates an ability score for each object, such that"/>
                <para PARENT="Default Paragraph Style"/>
                <para/>
                <ITEXT CH="for any pair of objects "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="i"/>
                <ITEXT CH=" and "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="j"/>
                <ITEXT CH="."/>
                <para/>
                <ITEXT CH="Citations between academic journals can be treated as paired comparisons: being cited means being an ‘exporter of intellectual influence’ (Stigler 1994; "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="Statistical Science"/>
                <ITEXT CH=")."/>
                <para/>
                <ITEXT CH="Using ability scores, we can predict the probability that journal "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="i"/>
                <ITEXT CH=" cites journal "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="j"/>
                <ITEXT CH=" more than "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="j"/>
                <ITEXT CH=" cites "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="i"/>
                <ITEXT CH=". Influential journals are more likely to be cited by other influential journals."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
