        <PAGEOBJECT XPOS="2011.94752" YPOS="2035.684" OwnPage="0" ItemID="106377392" PTYPE="4" WIDTH="399.98688" HEIGHT="1254.708" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="60" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="2" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L399.987 0 L399.987 1254.71 L0 1254.71 L0 0 Z" copath="M0 0 L399.987 0 L399.987 1254.71 L0 1254.71 L0 0 Z" gXpos="4378.51531586763" gYpos="1137.21862080398" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" Numeration="0"/>
                <ITEXT CH="Ranking journals"/>
                <para PARENT="Section heading"/>
                <ITEXT CH="How to measure influence using random walks"/>
                <para PARENT="Standfirst"/>
                <ITEXT CH="The "/>
                <ITEXT FONT="Goudy Old Style Bold" CH="PageRank algorithm"/>
                <ITEXT CH=" models the behaviour of a typical PhD student."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Open a random journal."/>
                <para PARENT="Enumerated list"/>
                <ITEXT CH="Pick a random reference and open the cited journal."/>
                <para PARENT="Enumerated list"/>
                <ITEXT CH="Repeat, "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="ad nauseum"/>
                <ITEXT CH="."/>
                <para PARENT="Enumerated list"/>
                <ITEXT CH="PageRank is the proportion of time spent reading each journal, i.e. the stationary distribution of an ergodic Markov chain. It is a measure of total influence."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="PageRank has a size bias: bigger journals have more/longer articles in them, attracting more citations. What if we want to measure "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="prestige"/>
                <ITEXT CH=", rather than "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="popularity"/>
                <ITEXT CH="?"/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="The "/>
                <ITEXT FONT="Goudy Old Style Bold" CH="Scroogefactor"/>
                <ITEXT CH=" score, defined as PageRank"/>
                <ITEXT FONT="Goudy Old Style Regular" CH=" "/>
                <ITEXT FONT="Goudy Old Style Italic" CH="per reference"/>
                <ITEXT CH=", controls for this size bias. It measures influence weight per outgoing citation."/>
                <para/>
                <ITEXT CH="Like the Bradley–Terry model, journals are, in effect, penalised for being generous with citations and rewarded for being miserly. When the Bradley–Terry model fits exactly, a journal's PageRank per reference is exactly equal to its ability score."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
